{
  "name": "kee-browser-addon",
  "displayName": "Kee - Password Manager",
  "description": "Kee adds private, secure and easy password management features which save time and keep your private data more secure.",
  "version": "3.10.0",
  "devDependencies": {
    "@ffflorian/jszip-cli": "^3.1.6",
    "@gorhill/publicsuffixlist": "^3.0.1",
    "@iconify-json/mdi": "^1.1.44",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.11.18",
    "@types/punycode": "^2.1.0",
    "@types/webextension-polyfill": "^0.10.0",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@unocss/reset": "^0.48.4",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/compiler-sfc": "^3.2.45",
    "@vue/test-utils": "^2.2.7",
    "@vueuse/core": "^9.11.1",
    "chokidar": "^3.5.3",
    "cross-env": "^7.0.3",
    "crx": "^5.0.1",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.9.0",
    "eslint-plugin-vuetify": "^2.0.0-beta.2",
    "esno": "^0.16.3",
    "fs-extra": "^11.1.0",
    "jest-webextension-mock": "^3.8.8",
    "jsdom": "^21.1.0",
    "kolorist": "^1.6.0",
    "linkify-element": "^4.1.0",
    "linkifyjs": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "pinia": "2.0.29",
    "prettier": "^2.8.3",
    "punycode": "^2.3.0",
    "rimraf": "^4.1.1",
    "serialize-error": "^11.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "unocss": "^0.48.4",
    "unplugin-auto-import": "^0.12.1",
    "unplugin-icons": "^0.15.1",
    "unplugin-vue-components": "^0.22.12",
    "vite": "^4.0.4",
    "vitest": "^0.28.3",
    "vue": "^3.2.45",
    "vue-demi": "^0.13.11",
    "vue-tsc": "^1.0.24",
    "vuetify": "^3.0.0",
    "web-ext": "^7.4.0",
    "webextension-polyfill": "^0.10.0"
  },
  "scripts": {
    "test": "vitest",
    "dev": "npm run clean && mkdir extension && cross-env NODE_ENV=development run-p dev:*",
    "dev:prepare": "esno scripts/prepare.ts",
    "dev:web": "vite",
    "dev:page": "npm run build:page -- --mode development",
    "dev:vault": "npm run build:vault -- --mode development",
    "build": "npm run clean && mkdir extension && cross-env NODE_ENV=production run-s build:web build:prepare build:page build:vault",
    "build:prepare": "esno scripts/prepare.ts",
    "build:web": "vite build",
    "build:page": "vite build --config vite.config.page.ts",
    "build:vault": "vite build --config vite.config.vault.ts",
    "pack": "cross-env NODE_ENV=production run-p pack:*",
    "pack:zip": "rimraf extension.zip && jszip-cli add extension/* -o ./extension.zip",
    "pack:crx": "crx pack extension -o ./extension.crx",
    "pack:xpi": "cross-env WEB_EXT_ARTIFACTS_DIR=./ web-ext build --source-dir ./extension --filename extension.xpi --overwrite-dest",
    "start:chromium": "web-ext run --source-dir ./extension --target=chromium",
    "start:firefox": "TMPDIR=~/tmp-dir/ web-ext run --source-dir ./extension --target=firefox-desktop --devtools --firefox=firefox",
    "clean": "rimraf extension",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "tsc": "vue-tsc --noEmit"
  }
}
