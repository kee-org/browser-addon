{
  "name": "kee-browser-addon",
  "displayName": "Kee - Password Manager",
  "description": "Kee adds private, secure and easy password management features which save time and keep your private data more secure.",
  "version": "3.12.0",
  "devDependencies": {
    "@ffflorian/jszip-cli": "^3.4.1",
    "@gorhill/publicsuffixlist": "^3.0.1",
    "@grapoza/vue-tree": "5.2.0",
    "@iconify-json/mdi": "^1.1.54",
    "@types/chrome": "^0.0.246",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.5.0",
    "@types/punycode": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@unocss/reset": "^0.55.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/compiler-sfc": "^3.3.4",
    "@vue/test-utils": "^2.4.1",
    "@vueuse/core": "^10.3.0",
    "chokidar": "^3.5.3",
    "cross-env": "^7.0.3",
    "crx": "^5.0.1",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "eslint-plugin-vuetify": "^2.0.5",
    "esno": "^0.17.0",
    "firefox-addons-add-update-version": "^1.0.1",
    "fs-extra": "^11.1.1",
    "jest-webextension-mock": "^3.8.9",
    "jsdom": "^22.1.0",
    "kolorist": "^1.8.0",
    "linkify-element": "^4.1.1",
    "linkifyjs": "^4.1.1",
    "npm-run-all": "^4.1.5",
    "pinia": "^2.1.6",
    "prettier": "^3.0.1",
    "punycode": "^2.3.0",
    "rimraf": "^5.0.1",
    "serialize-error": "^11.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "unocss": "^0.55.0",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-icons": "^0.16.5",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.4.9",
    "vitest": "^0.34.1",
    "vue": "^3.3.4",
    "vue-demi": "^0.14.5",
    "vue-tsc": "^1.8.8",
    "vuetify": "^3.3.12",
    "web-ext": "^7.6.2",
    "webext-detect-page": "^4.1.1"
  },
  "scripts": {
    "test": "vitest",
    "dev": "npm run clean && mkdir extension && cross-env NODE_ENV=development run-p dev:*",
    "dev-chrome": "npm run clean && mkdir extension && cross-env NODE_ENV=development BROWSER_TARGET=chrome run-p dev:*",
    "dev:prepare": "esno scripts/prepare.ts",
    "dev:background": "npm run build:background -- --mode development",
    "dev:web": "vite",
    "dev:page": "npm run build:page -- --mode development",
    "dev:vault": "npm run build:vault -- --mode development",
    "build": "npm run build:beta",
    "build:prod": "npm run clean && mkdir extension && cross-env NODE_ENV=production run-s build:web build:prepare build:page build:vault",
    "build:beta": "npm run clean && mkdir extension && cross-env NODE_ENV=production VITE_KEE_CHANNEL=beta run-s build:web build:prepare build:page build:vault",
    "build-chrome:prod": "npm run clean && mkdir extension && cross-env NODE_ENV=production BROWSER_TARGET=chrome run-s build:web build:prepare build:page build:vault",
    "build-chrome:beta": "npm run clean && mkdir extension && cross-env NODE_ENV=production BROWSER_TARGET=chrome VITE_KEE_CHANNEL=beta run-s build:web build:prepare build:page build:vault",
    "build:prepare": "esno scripts/prepare.ts",
    "build:background": "vite build --config vite.config.background.ts",
    "build:web": "vite build",
    "build:page": "vite build --config vite.config.page.ts",
    "build:vault": "vite build --config vite.config.vault.ts",
    "pack:prod": "cross-env NODE_ENV=production run-p pack:prod:xpi",
    "pack:beta": "cross-env NODE_ENV=production run-p pack:beta:xpi",
    "pack-chrome:prod": "cross-env NODE_ENV=production run-p pack:prod:zip",
    "pack-chrome:beta": "cross-env NODE_ENV=production run-p pack:beta:zip",
    "pack:prod:zip": "rimraf dist/extension.zip && jszip-cli add extension/* -o ./dist/extension.zip",
    "pack:prod:crx": "crx pack extension -o ./dist/extension.crx",
    "pack:prod:xpi": "cross-env WEB_EXT_ARTIFACTS_DIR=./dist/ web-ext build --source-dir ./extension --filename extension.xpi --overwrite-dest",
    "pack:beta:zip": "rimraf dist/extension-beta.zip && jszip-cli add extension/* -o ./dist/extension-beta.zip",
    "pack:beta:crx": "crx pack extension -o ./dist/extension-beta.crx",
    "pack:beta:xpi": "cross-env WEB_EXT_ARTIFACTS_DIR=./dist/ web-ext build --source-dir ./extension --filename extension-beta.xpi --overwrite-dest",
    "start:chrome": "web-ext run --source-dir ./extension --target=chromium",
    "start:firefox": "TMPDIR=~/tmp-dir/ web-ext run --source-dir ./extension --target=firefox-desktop --devtools --firefox=firefox",
    "clean": "rimraf extension",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "lint:xpi": "web-ext lint --source-dir ./extension",
    "sign:xpi:beta": "cross-env WEB_EXT_ARTIFACTS_DIR=./dist/signed web-ext sign --source-dir ./extension --channel=unlisted",
    "tsc": "vue-tsc --noEmit"
  }
}
